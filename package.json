{
  "name": "reportl",
  "version": "0.1.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "npm run db:up && nodemon server/server.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "db:up": "knex-migrate --cwd ./server/database/ up",
    "db:down": "knex-migrate --cwd ./server/database/ down",
    "db:drop": "knex-migrate --cwd ./server/database/ down --to 0",
    "db:empty": "npm run db:drop && npm run db:up",
    "db:rollback": "knex-migrate --cwd ./server/database/ rollback",
    "db:redo": "knex-migrate --cwd ./server/database/ redo",
    "db:seed": "knex --cwd ./server/database/ seed:run",
    "db:migrate:make": "knex --cwd ./server/database/ migrate:make",
    "db:seed:make": "knex --cwd ./server/database/ seed:make"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/closurecats/reportl.git"
  },
  "author": "Closure Cats",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/closurecats/reportl/issues"
  },
  "homepage": "https://github.com/closurecats/reportl#readme",
  "devDependencies": {
    "eslint": "^3.12.2",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.8.0",
    "nodemon": "^1.11.0"
  },
  "dependencies": {
    "bookshelf": "^0.10.2",
    "dotenv": "^2.0.0",
    "express": "^4.14.0",
    "knex": "^0.12.6",
    "knex-migrate": "^1.2.0",
    "mysql": "^2.12.0"
  }
}
